<?xml version="1.0" encoding="utf-8" ?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
  <item>
    <shape android:shape="rectangle">
      <solid android:color="@color/splash_background" />
      <padding
        android:left="0dip"
        android:top="0dip"
        android:right="0dip"
        android:bottom="0dip"/>
    </shape>
  </item>
  <item
    android:bottom="120dp"
    android:drawable="@drawable/spl_center"
    android:gravity="center"
    android:height="260dp"
    android:width="260dp">
  </item>
  <item
    android:bottom="45dp"
    android:drawable="@drawable/ic_splash"
    android:gravity="bottom | center_horizontal"
    android:height="60dp"
    android:width="225dp">
  </item>
</layer-list>
